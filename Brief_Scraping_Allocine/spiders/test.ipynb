    "import re\n",
    "\n",
    "def minutify(str):\n",
    "        h = int(re.findall(r'(\\d+)h.+', str)[0])\n",
    "        print(h)\n",
    "        m = int(re.findall(r'\\d+h (\\d\\d)m', str)[0])\n",
    "        print(m)\n",
    "        return (h*60)+m"
   ]
